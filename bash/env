# vim: set filetype=sh

export PATH=~/bin:/usr/local/bin:$PATH

# MacPorts Installer addition on 2010-10-02_at_09:10:22: adding an appropriate PATH variable for use with MacPorts.
export PATH=/opt/local/bin:/opt/local/sbin:$PATH
# Finished adapting your PATH environment variable for use with MacPorts.

# For Android SDK
export PATH=$PATH:/usr/local/share/android-sdk-mac_x86/tools:/usr/local/share/android-sdk-mac_x86/platform-tools

export EDITOR="mvim -f"
